class Animal { void eat() { System.out.println("Animal is eating..."); } }
class Dog extends Animal { void bark() { System.out.println("Dog is barking..."); } }
class BabyDog extends Dog { void weep() { System.out.println("Baby dog is weeping..."); } }

public class Main {
    public static void main(String[] args) {
        Dog dog = new Dog(); dog.eat(); dog.bark();
        BabyDog babyDog = new BabyDog(); babyDog.eat(); babyDog.bark(); babyDog.weep();
    }
}
