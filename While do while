public class Main {
    public static void main(String[] args) 
        int count = 0;
        while (count < 5) {
            System.out.println("Count is: " + count);
            count++;
        }
        
        System.out.println();
        int num = 0;
        do {
            System.out.println("Number is: " + num);
            num++;
        } while (num < 5);
    }
}
